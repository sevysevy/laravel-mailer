{
  "name": "sevysevy/laravel-mailer",
  "description": "Reusable email sender service for Laravel apps",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Sevysevy\\Mailer\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0",
    "illuminate/mail": "^10.0 || ^11.0 || ^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sevysevy\\Mailer\\MailerServiceProvider"
      ],
      "aliases": {
        "Mailer": "Sevysevy\\Mailer\\Facades\\Mailer"
      }
    }
  }
}
