{
  "name": "fakode/laravel-mailer",
  "description": "Reusable email sender service for Laravel apps",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Yves Ngah",
      "email": "youremail@example.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fakode\\Mailer\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "extra": {
    "laravel": {
